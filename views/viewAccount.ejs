<!DOCTYPE html>
<html lang="en">
    <!DOCTYPE html>
    <html lang="en">
    <head>
        <% include partials/head %>
    <body>
    <header>
        <% include partials/header %>
    </header>
    <div class="container">
        <hr>
        <div class="row">
            <div class="panel panel-primary filterable">
                <div class="panel-heading">
                    <h3 class="panel-title">Users</h3>
                </div>
                <table class="table">
                    <thead>
                        <tr>
                            <th><input type="text" class="form-control" placeholder="#" disabled></th>
                            <th><input type="text" class="form-control" placeholder="First Name" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Last Name" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Username" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Email" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Address" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Phone Number" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Vertify" disabled></th>
                            <th><input type="text" class="form-control" placeholder="Role" disabled></th>
                        </tr>
                    </thead>
                    <tbody>
                        <% order.forEach(function(u){ %>
                            <tr>
                                <td><%= u._id %></td>
                                <td><%= u.firstName %></td>
                                <td><%= u.lastName %></td>
                                <td><%= u.username %></td>
                                <td><%= u.email %></td>
                                <td><%= u.address %></td>
                                <td><%= u.phoneNumber %></td>
                                <td><%= u.isAuthenticated %></td>
                                <td><%= u.role %></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
<footer>
	<% include partials/footer %>
</footer>
    <% include partials/modal-addtocartproduct %>
    <% include partials/modal-quickviewmodal %>
    <% include partials/modal-videoproduct %>
    <% include partials/modal-subscribegood %>
    <% include partials/script %>
</body>
</html>